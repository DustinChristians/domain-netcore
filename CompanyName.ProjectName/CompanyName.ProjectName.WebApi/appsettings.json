{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "CompanyNameProjectNameContext": "Server=(localdb)\\mssqllocaldb;Database=CompanyNameProjectName;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "CompanyNameProjectNameContext",
          "tableName": "EventLog",
          "autoCreateSqlTable": true
        }
      }
    ]
  }
}
